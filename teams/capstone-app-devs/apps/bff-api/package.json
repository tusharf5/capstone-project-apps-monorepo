{
  "name": "bff-api",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.14 <17",
    "yarn": ">=1.22.0 <2"
  },
  "scripts": {
    "build": "npx webpack build --config ./webpack.config.js --no-stats",
    "lint": "yarn run prettier:fix && yarn run ts:check && yarn run eslint:check",
    "ts:check": "npx tsc --noEmit",
    "eslint:fix": "npx eslint src --ext ts,js --fix",
    "eslint:check": "npx eslint src --ext ts,js",
    "prettier:fix": "npx prettier --write '**/*.{ts,js,json,yml,yaml,md}'",
    "preinstall": "export PUPPETEER_SKIP_DOWNLOAD=1;"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "3.110.0",
    "@aws-sdk/client-dynamodb": "3.113.0",
    "@aws-sdk/client-ecs": "3.112.0",
    "@aws-sdk/client-s3": "3.113.0",
    "@aws-sdk/lib-dynamodb": "3.113.0",
    "@aws-sdk/lib-storage": "3.113.0",
    "@aws-sdk/signature-v4-crt": "3.110.0",
    "@aws-sdk/smithy-client": "3.110.0",
    "agentkeepalive": "4.2.1",
    "axios": "0.27.2",
    "fastify": "4.1.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.110.0",
    "@types/eslint": "8.4.1",
    "@types/node": "^16.0.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "clean-webpack-plugin": "4.0.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "prettier": "2.7.1",
    "ts-loader": "9.3.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.6.3",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  }
}
